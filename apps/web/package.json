{
  "name": "@repo/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --port 4000",
    "build": "next build",
    "build:worker": "opennextjs-cloudflare build --skipNextBuild",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare deploy",
    "deploy:preview": "opennextjs-cloudflare upload --preview-alias=$WEB_PREVIEW_ALIAS",
    "preview": "opennextjs-cloudflare preview",
    "cf:types": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next .open-next .turbo .wrangler"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.3.0",
    "@packages/api-contract": "workspace:*",
    "@packages/env": "workspace:*",
    "@packages/logger": "workspace:*",
    "@packages/schema": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-core": "^0.13.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "next": "^15.5.5",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19",
    "shadcn": "^3.5.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2",
    "wrangler": "^4.49.1"
  }
}
